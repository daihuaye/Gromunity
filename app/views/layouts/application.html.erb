<!DOCTYPE html>
<html>
<head>
  <title>TestApp</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="user_nav">
		<%- if current_user %>
			<%= "Signed in as #{current_user.email}. Not you?" %>
			<%= link_to "Sign out", logout_path %>
		<%- else %>
			<%= link_to "Sign in", new_user_session_path %>
		<%- end %>
	</div>
<%= yield %>

</body>
</html>
